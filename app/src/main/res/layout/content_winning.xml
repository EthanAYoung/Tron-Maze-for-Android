<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context=".WinningActivity"
    tools:showIn="@layout/activity_winning">

    <Button
        android:id="@+id/backButt2"
        android:layout_width="wrap_content"
        android:layout_height="35dp"
        android:layout_marginStart="161dp"
        android:layout_marginTop="16dp"
        android:onClick="returnToTitle"
        android:text="Back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/youWinText"
        android:layout_width="80dp"
        android:layout_height="41dp"
        android:layout_marginStart="165dp"
        android:layout_marginTop="141dp"
        android:text="YOU WIN"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/backButt2" />

    <TextView
        android:id="@+id/pathLenText"
        android:layout_width="134dp"
        android:layout_height="51dp"
        android:layout_marginStart="138dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="27dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/shortPathLenText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/shortPathLenText"
        android:layout_width="132dp"
        android:layout_height="51dp"
        android:layout_marginStart="140dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="30dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/engConsText"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/engConsText"
        android:layout_width="130dp"
        android:layout_height="51dp"
        android:layout_marginStart="142dp"
        android:layout_marginEnd="139dp"
        android:layout_marginBottom="116dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</android.support.constraint.ConstraintLayout>